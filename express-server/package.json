{
  "name": "express-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=production node ./bin/www",
    "start:dev": "cross-env NODE_ENV=development nodemon ./bin/www",
    "start:dev:debug": "cross-env NODE_ENV=development DEBUG=* nodemon ./bin/www",
    "start:inspect": "node --inspect ./bin/www"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "cross-env": "^7.0.3",
    "debug": "~2.6.9",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.7"
  }
}
